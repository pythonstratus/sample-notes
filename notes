SELECT 
    CAST(prior_four_quarter_average AS NUMERIC(38,10)) as prior_four_quarter_average,
    CAST(ratio_of_current_to_prior_4_avg AS NUMERIC(38,10)) as ratio_of_current_to_prior_4_avg,
    * 
FROM mcr.line_item_data
EXCEPT (prior_four_quarter_average, ratio_of_current_to_prior_4_avg)



SELECT 
    CAST(prior_four_quarter_average AS NUMERIC(38,10)) as prior_four_quarter_average,
    CAST(ratio_of_current_to_prior_4_avg AS NUMERIC(38,10)) as ratio_of_current_to_prior_4_avg,
    -- Select all columns except the two we're casting
    (SELECT * EXCEPT (prior_four_quarter_average, ratio_of_current_to_prior_4_avg) 
     FROM mcr.line_item_data)
FROM mcr.line_item_data;
